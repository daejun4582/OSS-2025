{
    "rewrites": [
        {
            "source": "/",
            "destination": "/hw1/index.html"
        },
        {
            "source": "/index.html",
            "destination": "/hw1/index.html"
        },
        {
            "source": "/:file(style1|style2|example).html",
            "destination": "/hw1/:file.html"
        },
        {
            "source": "/css/:path*",
            "destination": "/hw1/css/:path*"
        },
        {
            "source": "/hw2/:file(form_nostyle|form_style|form_js|form).html",
            "destination": "/hw2/:file.html"
        },
        {
            "source": "/hw2/css/:path*",
            "destination": "/hw2/css/:path*"
        },
        {
            "source": "/hw2-2/:file(index|add|edit|view).html",
            "destination": "/hw2-2/:file.html"
        },
        {
            "source": "/hw2-2/css/:path*",
            "destination": "/hw2-2/css/:path*"
        }
    ]
}